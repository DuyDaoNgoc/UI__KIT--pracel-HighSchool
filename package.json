{
  "name": "ui-kit-pracel",
  "version": "1.0.0",
  "scripts": {
    "predev": "npm run clean",
    "client": "parcel index.html --dist-dir dist --public-url / --port 5000 --host 0.0.0.0",
    "server": "cd server && ts-node-dev index.ts",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "build": "parcel build index.html --dist-dir dist --public-url https://UI-kit.com/",
    "clean": "rimraf .parcel-cache dist",
    "fix:svg": "node fix-svg-attrs.js"
  },
  "alias": {
    "assets": "./public/assets"
  },
  "proxy": {
    "/api": "http://UI-kit.com:5000"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-native-web": "^0.21.1",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.15.4",
    "@types/aos": "^3.0.7",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "nodemon": "^3.1.10",
    "parcel": "^2.15.4",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "posthtml": {
    "plugins": {}
  }
}
